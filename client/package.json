{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": {
    "email": "zach@saeon.ac.za",
    "name": "Zach Smith",
    "url": "http://saeon.ac.za"
  },
  "scripts": {
    "oncommit": "npm-run-all -s prettier lint",
    "onchange-watch": "onchange \"./**/*.@(js|jsx|css|scss)\" -- prettier --write {{changed}}",
    "start": "npm-run-all clean:dist copy:public start:dev",
    "start:dev": "npm-run-all -p onchange-watch serve:dev",
    "serve:dev": "webpack-dev-server --env.mode=development --open",
    "dist": "npm-run-all clean:dist copy:public pack",
    "clean:dist": "rimraf ./dist",
    "copy:public": "copyfiles -u 1 ./public/* dist",
    "pack": "webpack --env.mode=production",
    "pre-commit": "npm-run-all prettier lint",
    "prettier": "prettier --write \"src/**/*.@(js|jsx|css|scss)\"",
    "lint": "eslint \"./src/**/*.{js,jsx}\""
  },
  "dependencies": {
    "@apollo/client": "3.1.4",
    "@babel/core": "7.11.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@saeon/ol-react": "9.0.3",
    "babel-loader": "8.1.0",
    "classnames": "2.2.6",
    "copyfiles": "2.3.0",
    "core-js": "3.6.5",
    "css-loader": "4.2.2",
    "date-fns": "2.16.1",
    "dotenv-webpack": "2.0.0",
    "echarts": "4.9.0",
    "echarts-for-react": "2.0.16",
    "file-loader": "6.1.0",
    "graphql": "15.3.0",
    "graphql-tag": "2.11.0",
    "html-loader": "1.3.0",
    "html-webpack-plugin": "4.4.1",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "ol": "6.4.3",
    "query-string": "6.13.1",
    "ramda": "0.27.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-md": "1.16.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-transition-group": "4.4.1",
    "react-virtualized-auto-sizer": "1.0.2",
    "react-window": "1.8.5",
    "react-window-infinite-loader": "1.0.5",
    "regenerator-runtime": "0.13.7",
    "rimraf": "3.0.2",
    "sass-loader": "10.0.2",
    "sift": "13.3.1",
    "style-loader": "1.2.1",
    "url-loader": "4.1.0",
    "webfontloader": "1.6.28",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.8.1",
    "eslint-plugin-react": "7.20.6",
    "onchange": "7.0.2",
    "prettier": "2.1.1",
    "webpack-dev-server": "3.11.0"
  }
}
