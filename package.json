{
  "name": "api2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development npm-run-all server",
    "start-dist": "npm run prod",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "watch": "nodemon",
    "watch-prettier": "watch-prettier": "onchange '**/!(__generated__)/*.js' -- prettier --write {{changed}}"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.4.2",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  }
}
