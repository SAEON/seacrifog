{
  "name": "api2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "prettier": "prettier --write 'src/**/*.js'",
    "run:dev": "NODE_ENV=development npm-run-all server:dev",
    "server:dev": "babel-node ./src/bin",
    "start:prod": "NODE_ENV=production npm-run-all clean:prod build:prod copy:schema copy:env server:prod",
    "server:prod": "node ./dist/bin",
    "clean:prod": "rimraf ./dist",
    "build:prod": "babel ./src --out-dir dist",
    "copy:schema": "cp ./src/schema.graphql dist/schema.graphql",
    "copy:env": "cp ./.env dist/.env"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.4.2",
    "graphql-tools": "^4.0.5",
    "morgan": "~1.9.1",
    "pg": "^7.12.1",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2"
  }
}
